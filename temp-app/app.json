{
  "expo": {
    "name": "temp-app",
    "slug": "temp-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "static",
      "name": "Citizen Grievance System",
      "shortName": "GrievanceApp",
      "description": "Digital Platform for Government Services",
      "startUrl": "/",
      "display": "standalone",
      "orientation": "portrait",
      "themeColor": "#1DB954",
      "backgroundColor": "#000000"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
